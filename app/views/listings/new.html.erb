<div class="container">
  <%= simple_form_for @listing do |f| %>
  <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="input-field col s6">
            <%= f.input :price %>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            
            <%= f.input :description, input_html: { class: "materialize-textarea" } %>
            <!-- <textarea id="textarea1" class="materialize-textarea"></textarea> -->
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <%= f.input :address %>
          </div>
        </div>
        <!-- <div class="row">
          <div class="input-field col s3">
            <%= f.input :longitude %>
          </div>
          <div class="input-field col s3">
            <%= f.input :latitude %>
          </div>
        </div> -->
        <div class="row">
          <div class="input-field col s6">
            <%= f.input :topic %>
          </div>
        </div>
        <%= f.button :submit, class: "create-listing-btn"%>
        <%= link_to('Cancel', root_path, method: :get, class: "btn") %>
        
      </form>
    </div>
  <% end %>
</div>
  
